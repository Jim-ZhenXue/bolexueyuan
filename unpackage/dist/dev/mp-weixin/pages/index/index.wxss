
page {
		background: #FFFFFF;
		height: 100%;
		overflow: hidden;
}
.container {
		position: relative;
		width: 100%;
		height: 100vh;
		display: flex;
		flex-direction: column;
		background: #FFFFFF;
		overflow: hidden;
}
.top-area {
		background: #FFFFFF;
}
.header {
		width: 100%;
		height: 88rpx;
		display: flex;
		align-items: center;
		padding: 0 30rpx;
		background: #FFFFFF;
		/* box-shadow: 0 2rpx 10rpx rgba(73, 198, 111, 0.05); */
}
.logo {
		height: 56rpx;
		width: 56rpx;
		margin-right: 16rpx;
}
.title {
		font-size: 36rpx;
		font-weight: 600;
		color: #333333;
}
.scroll-container {
		flex: 1;
		width: 100%;
		background: #FFFFFF;
		overflow: hidden;
}
.image-list {
		padding: 30rpx;
		display: flex;
		flex-direction: column;
		gap: 30rpx;
		padding-bottom: 100rpx;
}
.image-item {
		width: 100%;
		height: 360rpx;
		border-radius: 24rpx;
		overflow: hidden;
		box-shadow: 0 8rpx 24rpx rgba(0, 0, 0, 0.1);
}
.item-image {
		width: 100%;
		height: 100%;
		object-fit: cover;
}
