
@font-face {
	font-family: "iconfont";
	src: url('data:font/woff2;charset=utf-8;base64,') format('woff2');
}
.container {
	min-height: 100vh;
	background-color: #000000;
}
.header {
	background: #000000;
}
.nav-bar {
	height: 88rpx;
	display: flex;
	align-items: center;
	position: relative;
	justify-content: center;
}
.nav-logo {
	width: 80rpx;
	height: 80rpx;
	filter: drop-shadow(0 0 5rpx rgba(49, 73, 50, 0.3));
}
.back-btn {
	position: absolute;
	left: 30rpx;
	top: 15rpx;
	height: 15rpx;
	width: 22rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.iconfont {
	font-family: "iconfont" !important;
	font-size: 15rpx;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	color: #FFFFFF;
}
.icon-home:before {
	content: "\e600";
}
.nav-title {
	font-size: 52rpx;
	font-weight: 600;
	color: #E0E0E0;
	text-shadow: 0 0 5rpx rgba(9, 143, 242, 0.3);
}
.nav-title .version-text {
	font-size: 26rpx;
	font-weight: normal;
}
.content {
	padding: 75rpx 20rpx 15rpx 20rpx;
	display: flex;
	flex-direction: column;
	align-items: center;
	background: #000000;
}
.logo {
	width: 160rpx;
	height: 160rpx;
	margin-bottom: 30rpx;
	filter: drop-shadow(0 0 10rpx rgba(76, 175, 80, 0.3));
}
.app-name {
	font-size: 20rpx;
	font-weight: 600;
	color: #E0E0E0;
	margin-bottom: 4rpx;
	text-shadow: 0 0 5rpx rgba(9, 143, 242, 0.3);
}
.version {
	font-size: 14rpx;
	color: #E0E0E0;
	margin-bottom: 15rpx;
}
.info-section {
	width: calc(100% - 100rpx);
	margin: 0 50rpx 12rpx 50rpx;
	background: rgba(255, 255, 255, 0.05);
	padding: 12rpx;
	border-radius: 10rpx;
	border: 1rpx solid rgba(255, 255, 255, 0.2);
}
.section-title {
	font-size: 16rpx;
	font-weight: 600;
	color: #E0E0E0;
	margin-bottom: 6rpx;
	display: block;
}
.description {
	font-size: 14rpx;
	color: #E0E0E0;
	line-height: 1.6;
	display: block;
}
.contact-item {
	font-size: 14rpx;
	color: #E0E0E0;
	line-height: 1.8;
	display: block;
}
.copyright-text {
	margin-top: 0;
	text-align: center;
}
.copyright-text text {
	font-size: 12rpx;
	color: #757575;
	display: block;
	line-height: 1.6;
}
.info-section.no-border {
	background: none;
	border: none;
	text-align: center;
}
.info-section.no-border .section-title,
.info-section.no-border .description {
	text-align: center;
	width: 100%;
}
.nav-content {
	display: flex;
	align-items: center;
	gap: 20rpx;
	margin-top: 50rpx;
}
.nav-title {
	font-size: 52rpx;
	font-weight: 600;
	color: #E0E0E0;
	text-shadow: 0 0 5rpx rgba(9, 143, 242, 0.3);
}
.nav-title .version-text {
	font-size: 26rpx;
	font-weight: normal;
}
.contact-row {
	display: flex;
	align-items: center;
	justify-content: center;
	gap: 30rpx;
	flex-wrap: wrap;
	width: 100%;
}
.contact-item {
	font-size: 14rpx;
	color: #E0E0E0;
	line-height: 1.8;
	text-align: center;
}
